  public class Professor extends Person{
  
  //instance variables
  private Person professor;
  private String title;
  private String department;
  private String teaches;
  
  public Professor()
  {
   professor = new Person();
   title = "UNASSIGNED";
   department = "UNASSIGNED";
   teaches = "UNASSIGNED";
  }
  
  //set title to inputed title
  public void setTitle(String title)
  {
    this.title = title;
  }
  
  //return title
  public String getTitle()
  {
    return title;
  }
  
  //set Department to inputed department
  public void setDepartment(String department)
  {
    this.department = department;
  }
  
  //return department
  public String getDepartment()
  {
    return department;
  }
  
  //display information
  public void display()
  { System.out.println("display information here");}
  
  public String toString()
  { 
    return "Professor" +professor+ " Title: " +title;
  }
  
  //display Teaching Assignments assigned to specific professor
  public void displayTeachingAssignments()
  {
  System.out.println("Display Information");}
  
  //assigns professor to teaching a section
 public void agreeToTeach()
  { 
 //add professor to teaches
 }
}
